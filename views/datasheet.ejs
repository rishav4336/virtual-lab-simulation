<%- include("partials/header") -%>

<div class="container text-center">
    <table id="table-1">
        <tr>
            <th>S.No</th>
            <th>τ (s)</th>
            <th>Xa (<%= temps[2] %>°C)</th>
            <th>Xa (<%= temps[0] %>°C)</th>
            <th>Xa (<%= temps[1] %>°C)</th>
        </tr>
        <% for(let i=0; i<tau_data.length; i++) { %>
            <tr>
                <td><%= i+1 %></td>
                <td><%= tau_data[i] %></td>
                <td><%= Xa_data[2][i].toPrecision(3) %></td>
                <td><%= Xa_data[0][i].toPrecision(3) %></td>
                <td><%= Xa_data[1][i].toPrecision(3) %></td>
            </tr>
        <% } %>
    </table>
</div>
<script>
    $('#table-1 tr td').addClass('border');
    $('#table-1 tr th').addClass('border');
</script>
<%- include("partials/footer") -%>
